        <attr name="line_visibility_iv" format="boolean" />
        <attr name="line_visibility_et" format="boolean" />

        <attr name="line_enabled" format="boolean" />

        <attr name="line_isdivider" format="boolean" />

        <attr name="line_tv_gravity" format="integer" />
        <attr name="line_et_gravity" format="integer" />
        <attr name="input_style" format="integer" />

    </declare-styleable>

    <declare-styleable name="ItemImageTextStyleable">
        <attr name="item_text" format="string" />
        <attr name="item_color" format="integer" />
        <attr name="item_size" format="integer" />
        <attr name="item_image" format="integer" />
    </declare-styleable>

    <declare-styleable name="hprogress">
        <attr name="progress" format="integer" />
        <attr name="max" format="integer" />
        <attr name="progressHeight" format="integer" />
        <attr name="progressColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="JessonTextColorOn" format="color" />
